@model IEnumerable<ApplicationUser>
@{
    ViewData["Title"] = "Users";
}
<h1>@ViewData["Title"]</h1>

<p>All Users on WearHouse</p>

@if (Model.Any())
{
foreach (var user in Model) {
    <div>
        <div>
            <h4>@user.FirstName @user.LastName</h4> 
            <h5>@user.UserName</h5>
        </div>
        <div>
            Email: @user.Email
        </div>
        <div>
            Location: @user.Location
        </div>
    </div>
    <br/>
}
}
else
{
    <p>There are no users on WearHouse. This is very sad...</p>
}
